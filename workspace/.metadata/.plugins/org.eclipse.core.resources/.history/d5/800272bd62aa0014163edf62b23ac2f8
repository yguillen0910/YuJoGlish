package com.tesisyux.realidadaumentada;

import android.app.Activity;
import android.content.Intent;
import android.os.Bundle;
import android.view.View;

public class LevelActivity extends Activity{
	
	final static String LEVEL_INFO = "com.tesisyux.realidadaumentada.LessonActivity"; // Ruta y Nombre de la activad a la cual voy a enviar la info.
	
	@Override
	protected void onCreate(Bundle savedInstanceState) {
		super.onCreate(savedInstanceState);
		setContentView(R.layout.activity_level);
		
		DataBaseManager manager = new DataBaseManager(this);
		
		
		manager.insertar("circle","1");
		manager.insertar("square","2");
		manager.insertar("rectangle","3");
		manager.insertar("triangle","4");
		manager.insertar("triangle","5");
		manager.insertar("triangle","6");
	
	}

	public void onPlayButtonClick(View v)
	{
		//setActiveModel(1);
		Intent myIntent = new Intent (this , LessonActivity.class);
		myIntent.putExtra(LEVEL_INFO, "Level 1");
		startActivity(myIntent);
	}
	
	public void onSettingsButtonClick(View v) {
		
		Intent myIntent = new Intent (this , LessonActivity.class);
		
		startActivity(myIntent);
		
	}
	
	
	


}
